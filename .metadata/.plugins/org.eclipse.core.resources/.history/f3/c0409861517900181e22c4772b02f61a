package message;

public class MessagePackage {
	Pieces pies;
	public byte[] pack(String charsetName) {
		int index;
		byte[] result = new byte[this.getLength];
		
	}
	
	public int getLength()
	{
		int ret = 0;
		for (Pieces pie: pies )
			ret += pie.getLength();
	}
	public static void main(String[] args) {}

}
